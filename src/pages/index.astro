---
import Layout from '../layouts/Layout.astro';
import { getCollection } from 'astro:content';

// Get all home content
const heroEntries = await getCollection('home', ({ id }) => id === 'hero.md');
const hero = heroEntries[0]?.data || {};

const aboutEntries = await getCollection('home', ({ id }) => id === 'about.md');
const about = aboutEntries[0]?.data || {};

const subjectsEntries = await getCollection('home', ({ id }) => id === 'subjects.md');
const subjectsData = subjectsEntries[0]?.data || {};

const pricingEntries = await getCollection('home', ({ id }) => id === 'pricing.md');
const pricing = pricingEntries[0]?.data || {};

const contactEntries = await getCollection('home', ({ id }) => id === 'contact.md');
const contact = contactEntries[0]?.data || {};

const infoEntries = await getCollection('home', ({ id }) => id === 'info.md');
const info = infoEntries[0]?.data || {};

const footerEntries = await getCollection('home', ({ id }) => id === 'footer.md');
const footer = footerEntries[0]?.data || {};

const enrolmentEntries = await getCollection('home', ({ id }) => id === 'enrolment.md');
const enrolment = enrolmentEntries[0]?.data || {};

const settingsEntries = await getCollection('settings', ({ id }) => id === 'site.yaml');
const settings = settingsEntries[0]?.data || {};
---

<Layout title="TallyUpTutors | Mathematics, Science & Accounting Tuition">
	<main>
		<!-- AMBER GLOW BACKGROUND -->
		<div class="bg-amber-glow"></div>

		<!-- HEADER -->
		<header class="site-header">
			<div class="container">
				<a href="#" class="logo">
					<span class="logo-main">{settings.siteName || 'TALLYUP'}</span>
					<span class="logo-sub">TUTORS</span>
				</a>
				<nav class="main-nav">
					{settings.navLinks?.map((link) => (
						<a href={link.url}>{link.label}</a>
					)) || (
						<>
							<a href="#about">About</a>
							<a href="#subjects">Subjects</a>
							<a href="#pricing">Pricing</a>
							<a href="#contact">Contact</a>
						</>
					)}
				</nav>
			</div>
		</header>

		<!-- HERO -->
		<section id="hero" class="hero">
			<div class="container">
				<div class="hero-content">
					<div class="hero-badge">
						<span class="badge-dot"></span>
						{hero.freeTrialBadge || 'Free Trial Lessons Available'}
					</div>
					<h1 class="hero-title">
						<span class="title-animate">
							{(hero.heroTitle || 'TALLYUP TUTORS').split('').map((char, i) => (
								<span class="title-char" style={`--delay: ${i * 50}ms`}>{char}</span>
							))}
						</span>
					</h1>
					<p class="hero-subtitle">{hero.heroSubtitle || 'Expert Tuition in Mathematics, Science & Accounting'}</p>
					<p class="hero-description">
						{hero.heroDescription || 'Tally Up is a dynamic tuition centre dedicated to enhancing academic performance. We provide personalised, high-quality instruction tailored to each student\'s learning style.'}
					</p>
					<div class="hero-actions">
						<a href="#contact" class="btn btn-accent">
							<span>Book Free Trial</span>
							<svg xmlns="http://www.w3.org/2000/svg" width="18" height="18" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round">
								<line x1="5" y1="12" x2="19" y2="12"></line>
								<polyline points="12 5 19 12 12 19"></polyline>
							</svg>
						</a>
						<a href={settings.whatsapp || "https://wa.me/27718224498"} class="btn btn-whatsapp">
							<svg xmlns="http://www.w3.org/2000/svg" width="18" height="18" viewBox="0 0 24 24" fill="currentColor">
								<path d="M17.472 14.382c-.297-.149-1.758-.867-2.03-.967-.273-.099-.471-.148-.67.15-.197.297-.767.966-.94 1.164-.173.199-.347.223-.644.075-.297-.15-1.255-.463-2.39-1.475-.883-.788-1.48-1.761-1.653-2.059-.173-.297-.018-.458.13-.606.134-.133.298-.347.446-.52.149-.174.198-.298.298-.497.099-.198.05-.371-.025-.52-.075-.149-.669-1.612-.916-2.207-.242-.579-.487-.5-.669-.51-.173-.008-.371-.01-.57-.01-.198 0-.52.074-.792.372-.272.297-1.04 1.016-1.04 2.479 0 1.462 1.067 2.877 1.215 3.076.149.198 2.096 3.2 5.077 4.487.709.306 1.262.489 1.694.625.712.227 1.36.195 1.871.118.571-.085 1.758-.719 2.006-1.413.248-.694.248-1.289.173-1.413-.074-.124-.272-.198-.57-.347m-5.421 7.403h-.004a9.87 9.87 0 01-5.031-1.378l-.361-.214-3.741.982.998-3.648-.235-.374a9.86 9.86 0 01-1.51-5.26c.001-5.45 4.436-9.884 9.888-9.884 2.64 0 5.122 1.03 6.988 2.898a9.825 9.825 0 012.893 6.994c-.003 5.45-4.437 9.884-9.885 9.884m8.413-18.297A11.815 11.815 0 0012.05 0C5.495 0 .16 5.335.157 11.892c0 2.096.547 4.142 1.588 5.945L.057 24l6.305-1.654a11.882 11.882 0 005.683 1.448h.005c6.554 0 11.89-5.335 11.893-11.893a11.821 11.821 0 00-3.48-8.413Z"/>
							</svg>
							<span>WhatsApp Us</span>
						</a>
					</div>
					<div class="hero-stats">
						{hero.stats?.map((stat) => (
							<div class="stat">
								<span class="stat-value">{stat.value}</span>
								<span class="stat-label">{stat.label}</span>
							</div>
						)) || (
							<>
								<div class="stat">
									<span class="stat-value">95%</span>
									<span class="stat-label">Pass Rate</span>
								</div>
								<div class="stat">
									<span class="stat-value">500+</span>
									<span class="stat-label">Students</span>
								</div>
								<div class="stat">
									<span class="stat-value">8-12</span>
									<span class="stat-label">Grades</span>
								</div>
							</>
						)}
					</div>
				</div>
			</div>
		</section>

		<!-- ABOUT / WHAT WE OFFER -->
		<section id="about" class="about">
			<div class="container">
				<div class="about-grid">
					<div class="about-content">
						<div class="section-badge">Why Choose Us</div>
						<h2>{about.title || 'What We Offer'}</h2>
						<p class="about-intro">{about.intro || 'Expert tuition in a friendly, focused environment designed to help your child succeed.'}</p>
						<ul class="offer-list">
							{about.features?.map((feature) => (
								<li>
									<span class="offer-icon">
										<svg xmlns="http://www.w3.org/2000/svg" width="18" height="18" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2">
											<polyline points="20 6 9 17 4 12"></polyline>
										</svg>
									</span>
									<span>{feature}</span>
								</li>
							)) || (
								<>
									<li><span class="offer-icon"><svg xmlns="http://www.w3.org/2000/svg" width="18" height="18" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2"><polyline points="20 6 9 17 4 12"></polyline></svg></span><span>Expert tutoring in maths, science & accounting</span></li>
									<li><span class="offer-icon"><svg xmlns="http://www.w3.org/2000/svg" width="18" height="18" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2"><polyline points="20 6 9 17 4 12"></polyline></svg></span><span>Tailored support for Grade 8-12 learners</span></li>
									<li><span class="offer-icon"><svg xmlns="http://www.w3.org/2000/svg" width="18" height="18" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2"><polyline points="20 6 9 17 4 12"></polyline></svg></span><span>Friendly, focused sessions in Boksburg</span></li>
									<li><span class="offer-icon"><svg xmlns="http://www.w3.org/2000/svg" width="18" height="18" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2"><polyline points="20 6 9 17 4 12"></polyline></svg></span><span>Passionate educators who care</span></li>
									<li><span class="offer-icon"><svg xmlns="http://www.w3.org/2000/svg" width="18" height="18" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2"><polyline points="20 6 9 17 4 12"></polyline></svg></span><span>Proven strategies for exam success</span></li>
									<li><span class="offer-icon"><svg xmlns="http://www.w3.org/2000/svg" width="18" height="18" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2"><polyline points="20 6 9 17 4 12"></polyline></svg></span><span>Clear progress tracking for parents & learners</span></li>
								</>
							)}
						</ul>
					</div>
					<div class="about-info">
						<div class="info-card">
							<h3>Office Hours</h3>
							<table class="hours-table">
								{info.officeHours?.map((hour) => (
									<tr>
										<td>{hour.day}</td>
										<td>{hour.time}</td>
									</tr>
								)) || (
									<>
										<tr><td>Monday - Thursday</td><td>10h00 - 18h00</td></tr>
										<tr><td>Friday</td><td>10h00 - 16h00</td></tr>
										<tr><td>Saturday</td><td>08h00 - 12h00</td></tr>
									</>
								)}
							</table>
						</div>
						<div class="info-card">
							<h3>Visit Us</h3>
							<p class="address">{info.address || '184 Konig Street, Boksburg South, 1459'}</p>
							<div class="contact-phones">
								<a href={`tel:${info.phone1?.replace(/\s/g, '') || '0718224498'}`} class="phone-link">
									<svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2">
										<path d="M22 16.92v3a2 2 0 0 1-2.18 2 19.79 19.79 0 0 1-8.63-3.07 19.5 19.5 0 0 1-6-6 19.79 19.79 0 0 1-3.07-8.67A2 2 0 0 1 4.11 2h3a2 2 0 0 1 2 1.72 12.84 12.84 0 0 0 .7 2.81 2 2 0 0 1-.45 2.11L8.09 9.91a16 16 0 0 0 6 6l1.27-1.27a2 2 0 0 1 2.11-.45 12.84 12.84 0 0 0 2.81.7A2 2 0 0 1 22 16.92z"></path>
									</svg>
									{info.phone1 || '071 822 4498'}
								</a>
								<a href={`tel:${info.phone2?.replace(/\s/g, '') || '0722537197'}`} class="phone-link">
									<svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2">
										<path d="M22 16.92v3a2 2 0 0 1-2.18 2 19.79 19.79 0 0 1-8.63-3.07 19.5 19.5 0 0 1-6-6 19.79 19.79 0 0 1-3.07-8.67A2 2 0 0 1 4.11 2h3a2 2 0 0 1 2 1.72 12.84 12.84 0 0 0 .7 2.81 2 2 0 0 1-.45 2.11L8.09 9.91a16 16 0 0 0 6 6l1.27-1.27a2 2 0 0 1 2.11-.45 12.84 12.84 0 0 0 2.81.7A2 2 0 0 1 22 16.92z"></path>
									</svg>
									{info.phone2 || '072 253 7197'}
								</a>
							</div>
						</div>
					</div>
				</div>
			</div>
		</section>

		<!-- SUBJECTS -->
		<section id="subjects" class="subjects">
			<div class="container">
				<div class="section-badge">Our Subjects</div>
				<h2 class="section-title">{subjectsData.title || 'What We Teach'}</h2>
				<div class="subjects-grid">
					{subjectsData.subjects?.map((subject) => (
						<div class={`subject-card ${subject.featured ? 'featured' : ''}`}>
							{subject.featured && <div class="featured-badge">Most Popular</div>}
							<div class="subject-icon">
								<svg xmlns="http://www.w3.org/2000/svg" width="48" height="48" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="1.5">
									<line x1="12" y1="5" x2="12" y2="19"/><line x1="5" y1="12" x2="19" y2="12"/>
								</svg>
							</div>
							<h3>{subject.title}</h3>
							<p class="grade-range">{subject.gradeRange}</p>
							<div class="subject-tags">
								{subject.tags.map((tag) => <span>{tag}</span>)}
							</div>
						</div>
					)) || (
						<>
							<div class="subject-card">
								<div class="subject-icon">
									<svg xmlns="http://www.w3.org/2000/svg" width="48" height="48" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="1.5">
										<path d="M9 3h6v2H9zm0 16h6v2H9zM4 11h16v2H4z"/><circle cx="12" cy="12" r="3"/>
									</svg>
								</div>
								<h3>Science & Accounting</h3>
								<p class="grade-range">Grades 10-12</p>
								<div class="subject-tags">
									<span>Physical Sciences</span><span>Accounting</span>
								</div>
							</div>
							<div class="subject-card featured">
								<div class="featured-badge">Most Popular</div>
								<div class="subject-icon">
									<svg xmlns="http://www.w3.org/2000/svg" width="48" height="48" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="1.5">
										<line x1="12" y1="5" x2="12" y2="19"/><line x1="5" y1="12" x2="19" y2="12"/>
									</svg>
								</div>
								<h3>Mathematics</h3>
								<p class="grade-range">Grades 8-12</p>
								<div class="subject-tags">
									<span>Core Maths</span><span>Math Lit</span>
								</div>
							</div>
						</>
					)}
				</div>
			</div>
		</section>

		<!-- PRICING -->
		<section id="pricing" class="pricing">
			<div class="container">
				<div class="section-badge">Pricing</div>
				<h2 class="section-title">{pricing.title || 'Flexible Price Plan'}</h2>
				<p class="pricing-intro">{pricing.intro || 'Based on weekly hours ranging from 2 to 6 hours, billed monthly. More hours = better value.'}</p>
				<div class="pricing-cards">
					{pricing.pricingCards?.map((card) => (
						<div class={`pricing-card ${card.featured ? 'featured' : ''}`}>
							<div class="card-header">
								<span class="hours-label">{card.hours}</span>
							</div>
							<div class="card-body">
								<div class="rate">R{card.rate}<span class="rate-unit">/hr</span></div>
								<div class="total">
									<span class="total-label">Monthly Total</span>
									<span class="total-amount">R{card.monthlyTotal.toLocaleString()}</span>
								</div>
								<div class="monthly-hours">{card.monthlyHours}</div>
							</div>
						</div>
					)) || (
						<>
							<div class="pricing-card">
								<div class="card-header"><span class="hours-label">2 Hours / Week</span></div>
								<div class="card-body">
									<div class="rate">R220<span class="rate-unit">/hr</span></div>
									<div class="total"><span class="total-label">Monthly Total</span><span class="total-amount">R1,760</span></div>
									<div class="monthly-hours">8 hours / month</div>
								</div>
							</div>
							<div class="pricing-card">
								<div class="card-header"><span class="hours-label">3 Hours / Week</span></div>
								<div class="card-body">
									<div class="rate">R190<span class="rate-unit">/hr</span></div>
									<div class="total"><span class="total-label">Monthly Total</span><span class="total-amount">R2,280</span></div>
									<div class="monthly-hours">12 hours / month</div>
								</div>
							</div>
							<div class="pricing-card featured">
								<div class="card-header"><span class="hours-label">4 Hours / Week</span></div>
								<div class="card-body">
									<div class="rate">R160<span class="rate-unit">/hr</span></div>
									<div class="total"><span class="total-label">Monthly Total</span><span class="total-amount">R2,550</span></div>
									<div class="monthly-hours">18 hours / month</div>
								</div>
							</div>
							<div class="pricing-card">
								<div class="card-header"><span class="hours-label">5 Hours / Week</span></div>
								<div class="card-body">
									<div class="rate">R130<span class="rate-unit">/hr</span></div>
									<div class="total"><span class="total-label">Monthly Total</span><span class="total-amount">R2,600</span></div>
									<div class="monthly-hours">20 hours / month</div>
								</div>
							</div>
							<div class="pricing-card">
								<div class="card-header"><span class="hours-label">6 Hours / Week</span></div>
								<div class="card-body">
									<div class="rate">R110<span class="rate-unit">/hr</span></div>
									<div class="total"><span class="total-label">Monthly Total</span><span class="total-amount">R2,640</span></div>
									<div class="monthly-hours">24 hours / month</div>
								</div>
							</div>
						</>
					)}
				</div>
			</div>
		</section>

		<!-- TAGLINE -->
		<section class="tagline">
			<div class="container text-center">
				<p class="tagline-text">
					<span class="tagline-word">Let's</span>
					<span class="tagline-word">tally</span>
					<span class="tagline-word">up</span>
					<span class="tagline-word">your</span>
					<span class="tagline-word">success</span>
					<span class="tagline-word">.</span>
				</p>
			</div>
		</section>

		<!-- ENROLMENT -->
		<section id="enrolment" class="enrolment">
			<div class="container text-center">
				<div class="enrol-badge">{enrolment.badge || '2026 Enrollment'}</div>
				<h2 class="enrol-title">{enrolment.title || 'ENROLL NOW'}</h2>
				<p class="enrol-tagline">{enrolment.tagline || "Let's tally up your success."}</p>
				<a href="#contact" class="btn btn-accent btn-lg">
					<span>Secure Your Spot</span>
					<svg xmlns="http://www.w3.org/2000/svg" width="20" height="20" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round">
						<line x1="5" y1="12" x2="19" y2="12"></line>
						<polyline points="12 5 19 12 12 19"></polyline>
					</svg>
				</a>
			</div>
		</section>

		<!-- CONTACT FORM -->
		<section id="contact" class="contact">
			<div class="container">
				<div class="section-badge">Get In Touch</div>
				<h2 class="section-title">{contact.title || 'Contact Us'}</h2>
				<div class="contact-grid">
					<div class="contact-info">
						<p class="contact-intro">{contact.intro || 'Ready to get started? Fill out the form and we\'ll be in touch within 24 hours.'}</p>
						<address class="address-box">
							<svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2">
								<path d="M21 10c0 7-9 13-9 13s-9-6-9-13a9 9 0 0 1 18 0z"></path>
								<circle cx="12" cy="10" r="3"></circle>
							</svg>
							<span>{contact.address || '184 Konig Street, Boksburg South, 1459'}</span>
						</address>
						<div class="contact-phones">
							<a href={`tel:${contact.phone1?.replace(/\s/g, '') || '0718224498'}`} class="phone-btn">
								<svg xmlns="http://www.w3.org/2000/svg" width="20" height="20" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2">
									<path d="M22 16.92v3a2 2 0 0 1-2.18 2 19.79 19.79 0 0 1-8.63-3.07 19.5 19.5 0 0 1-6-6 19.79 19.79 0 0 1-3.07-8.67A2 2 0 0 1 4.11 2h3a2 2 0 0 1 2 1.72 12.84 12.84 0 0 0 .7 2.81 2 2 0 0 1-.45 2.11L8.09 9.91a16 16 0 0 0 6 6l1.27-1.27a2 2 0 0 1 2.11-.45 12.84 12.84 0 0 0 2.81.7A2 2 0 0 1 22 16.92z"></path>
								</svg>
								{contact.phone1 || '071 822 4498'}
							</a>
							<a href={`tel:${contact.phone2?.replace(/\s/g, '') || '0722537197'}`} class="phone-btn">
								<svg xmlns="http://www.w3.org/2000/svg" width="20" height="20" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2">
									<path d="M22 16.92v3a2 2 0 0 1-2.18 2 19.79 19.79 0 0 1-8.63-3.07 19.5 19.5 0 0 1-6-6 19.79 19.79 0 0 1-3.07-8.67A2 2 0 0 1 4.11 2h3a2 2 0 0 1 2 1.72 12.84 12.84 0 0 0 .7 2.81 2 2 0 0 1-.45 2.11L8.09 9.91a16 16 0 0 0 6 6l1.27-1.27a2 2 0 0 1 2.11-.45 12.84 12.84 0 0 0 2.81.7A2 2 0 0 1 22 16.92z"></path>
								</svg>
								{contact.phone2 || '072 253 7197'}
							</a>
						</div>
